<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <solid android:color="@color/gh" />
            <stroke android:width="1.0dip" android:color="@color/g7" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <solid android:color="@color/gj" />
            <stroke android:width="1.0dip" android:color="@color/g9" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <solid android:color="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ffeeeeee" />
        </shape>
    </item>
</selector>
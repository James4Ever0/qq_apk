        <shape android:shape="rectangle">
            <corners android:radius="@dimen/tv" />
            <solid android:color="#19000000" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/tv" />

        "padding_right":"3",
        "margin_left":"-9",
        "margin_bottom":"-9",
        "height":{
          "type":"relative",
          "value":"12"
        },
        "width":{
          "type":"wrap_content"
        },
        "attributes":{
          "setText:":"$comment_count",
          "setTextColorString:":"$count_txt_color",
          "setBackgroundColorString:":"$count_bg_color",
          "setCornerRadiusString:":"6",
          "setTextAlignmentString:":"1",
          "setAndroidFontFilePath:iOSFontName:size:":[
            "",
            "PingFangSC-Medium",
            "10"
          ]
        }
      },
      {
        "view_type":"UIButton",

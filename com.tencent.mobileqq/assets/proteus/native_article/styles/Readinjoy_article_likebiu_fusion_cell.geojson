        "margin_left":"26",
        "height":{
          "type":"relative",
          "value":"24"
        },
        "width":{
          "type":"relative",
          "value":"24"
        },
        "attributes":{
          "setImages:colors:direction:forStates:":[
            ["$biu_normalstate_image","read_in_joy_pressed_biu"],
            ["#000000","#000000"],
            ["0","0"],
            ["0","1"]
            ]
        }

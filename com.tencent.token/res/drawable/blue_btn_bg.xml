<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ff095cc1" android:endColor="#ff095cc1" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff095cc1" android:endColor="#ff095cc1" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff247fed" android:endColor="#ff247fed" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
</selector>